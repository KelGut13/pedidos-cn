{
  "name": "pedidoscn-dev",
  "version": "1.0.0",
  "description": "Script para ejecutar frontend y backend juntos",
  "scripts": {
    "start-frontend": "cd frontend && npm start",
    "start-backend": "cd backend && node server.js",
